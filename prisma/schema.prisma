generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CpuUsage {
  id    Int    @id @default(autoincrement())
  time  Int
  usage Float
}

model MemoryUsage {
  id                Int    @id @default(autoincrement())
  time              Int
  totalMemMb        Float
  usedMemMb         Float
  freeMemMb         Float
  freeMemPercentage Float
}
